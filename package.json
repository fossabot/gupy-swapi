  "scripts": {
    "seed": "npx babel-node db/seed.js",
    "gupy-swapi": "npx pm2 start ecosystem.config.js --only gupy-swapi",
    "gupy-swapi-dev": "npx pm2 start ecosystem.config.js --only gupy-swapi-dev",
